{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "web/**",
      "use": "@vercel/static"
    },
    {
      "src": "Media/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/Media/(.*)",
      "dest": "/Media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "web/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
