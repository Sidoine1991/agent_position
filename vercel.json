{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node@3.0.7"
    },
    {
      "src": "web/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/geo/departements",
      "dest": "/api/geo-departements"
    },
    {
      "src": "/api/geo/communes",
      "dest": "/api/geo-communes"
    },
    {
      "src": "/api/geo/arrondissements",
      "dest": "/api/geo-arrondissements"
    },
    {
      "src": "/api/geo/villages",
      "dest": "/api/geo-villages"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ]
}
