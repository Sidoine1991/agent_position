{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" },
    { "src": "api/handler.js", "use": "@vercel/node" },
    { "src": "web/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/Media/(.*)", "dest": "/web/Media/$1" },
    { "src": "/", "dest": "/web/home.html" },
    { "src": "/(.*)", "dest": "/web/$1" }
  ]
}


